{
    "name": "webpack-alioss-plugin",
    "version": "1.4.5",
    "description": "Ali oss-webpack auto upload files plugin ",
    "main": "src/index.js",
    "repository": "https://github.com/borenXue/webpack-alioss-plugin.git",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/borenXue/webpack-alioss-plugin/issues/new"
    },
    "scripts": {
        "dev": "./node_modules/.bin/cross-env NODE_ENV=development && webpack-dev-server --inline --hot --colors --progress --config build/webpack.conf.js",
        "build": "./node_modules/.bin/cross-env NODE_ENV=production && ./node_modules/.bin/webpack --config build/webpack.conf.js",
        "eslint": "./node_modules/.bin/eslint --format node_modules/eslint-friendly-formatter src/index.js",
        "eslint:html": "./node_modules/.bin/eslint --format html ./src/* -o ./dist/eslint/index.html",
        "appveyor:test": "./node_modules/.bin/eslint --format node_modules/eslint-friendly-formatter src/index.js",
        "travis:test": "npm run eslint"
    },
    "engines": {
        "node": ">=4.0.0",
        "npm": ">= 3.0.0"
    },
    "homepage": "http://www.xueboren.com",
    "author": "borenXue <boren.xue@outlook.com> (http://www.xueboren.com)",
    "dependencies": {
        "address": "^1.0.1",
        "ali-oss": "^4.8.0",
        "chalk": "^1.1.3",
        "co": "^4.6.0",
        "lodash": "^4.17.4"
    },
    "devDependencies": {
        "babel": "^6.23.0",
        "babel-core": "^6.24.1",
        "babel-eslint": "^7.2.3",
        "babel-loader": "^7.0.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "babel-runtime": "^6.26.0",
        "cross-env": "^4.0.0",
        "eslint": "^4.13.0",
        "eslint-config-standard": "^10.2.1",
        "eslint-friendly-formatter": "^2.0.7",
        "eslint-loader": "^1.9.0",
        "eslint-plugin-import": "^2.8.0",
        "eslint-plugin-node": "^4.2.2",
        "eslint-plugin-promise": "^3.5.0",
        "eslint-plugin-standard": "^3.0.1",
        "uglify-js": "^2.8.22",
        "uglifyjs-webpack-plugin": "^0.4.3",
        "webpack": "^2.4.1"
    }
}
