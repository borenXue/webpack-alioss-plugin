{
  "name": "webpack-alioss-plugin",
  "version": "2.4.2",
  "description": "Ali oss-webpack auto upload files plugin ",
  "homepage": "http://www.xueboren.com",
  "bugs": {
    "url": "https://github.com/borenXue/webpack-alioss-plugin/issues/new"
  },
  "repository": "https://github.com/borenXue/webpack-alioss-plugin.git",
  "license": "MIT",
  "author": "borenXue <boren.xue@outlook.com> (http://www.xueboren.com)",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "docs": "cp README.md docs && ./node_modules/gh-pages/bin/gh-pages.js -d docs --silent --repo https://${GITHUB_TOKEN}@github.com/borenXue/webpack-alioss-plugin.git",
    "lint": "npm-run-all --parallel lint:*",
    "lint:editconfig": "eclint check --block_comment_start '/**' --block_comment_end '*/'",
    "lint:eslint": "eslint --ext .js src --format html -o ./docs/eslint.html",
    "semantic-release": "semantic-release",
    "test": "mocha test --recursive --require babel-core/register --reporter mochawesome --reporter-options reportDir=./docs/mocha,reportName=webpack-alioss-plugin,reportTitle=webpack-alioss-plugin,inlineAssets=true"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*": [
      "eclint fix --block_comment_start '/**' --block_comment_end '*/'",
      "git add",
      "eclint check --block_comment_start '/**' --block_comment_end '*/'"
    ],
    "package.json": [
      "sort-package-json",
      "git add"
    ],
    "src/**/*.{js,vue}": [
      "eslint --fix --ext .js,.vue src",
      "git add",
      "npm run lint:eslint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "ali-oss": "^6.1.0",
    "babel-runtime": "^6.26.0",
    "chalk": "^2.4.2",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.4.0",
    "@commitlint/config-conventional": "^7.3.1",
    "@semantic-release/exec": "^3.3.2",
    "@semantic-release/git": "^7.0.8",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "conventional-changelog-cli": "^2.0.11",
    "cz-conventional-changelog": "^2.1.0",
    "eclint": "^2.8.1",
    "eslint": "^5.12.1",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.1",
    "mocha": "^5.2.0",
    "mochawesome": "^3.1.1",
    "npm-run-all": "^4.1.5",
    "semantic-release": "^19.0.3",
    "sort-package-json": "^1.18.0"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "release": {
    "branch": "master",
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES"
            ]
          },
          "releaseRules": [
            {
              "type": "feat",
              "release": "minor"
            },
            {
              "type": "fix",
              "release": "patch"
            },
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            },
            {
              "type": "refactor",
              "scope": "src",
              "release": "major"
            },
            {
              "type": "perf",
              "release": "minor"
            },
            {
              "type": "test",
              "release": "patch"
            },
            {
              "type": "build",
              "release": "patch"
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES"
            ]
          },
          "writerOpts": {
            "commitsSort": [
              "scope",
              "subject"
            ]
          }
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      [
        "@semantic-release/exec",
        {
          "prepare": "pwd && ls && git status && git diff",
          "publishCmd": "npm run docs"
        }
      ]
    ]
  }
}
